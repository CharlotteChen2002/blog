else
  export NODE_PATH="/Users/hibiki/CharlotteChenblog/Q-Blog/node_modules/.pnpm/@antfu+eslint-config@3.0.0_@typescript-eslint+utils@8.3.0_eslint@9.9.1_jiti@1.21.3__typescrip_rxgiowk5oe6yk2k63vvhvsky2i/node_modules/@antfu/eslint-config/bin/node_modules:/Users/hibiki/CharlotteChenblog/Q-Blog/node_modules/.pnpm/@antfu+eslint-config@3.0.0_@typescript-eslint+utils@8.3.0_eslint@9.9.1_jiti@1.21.3__typescrip_rxgiowk5oe6yk2k63vvhvsky2i/node_modules/@antfu/eslint-config/node_modules:/Users/hibiki/CharlotteChenblog/Q-Blog/node_modules/.pnpm/@antfu+eslint-config@3.0.0_@typescript-eslint+utils@8.3.0_eslint@9.9.1_jiti@1.21.3__typescrip_rxgiowk5oe6yk2k63vvhvsky2i/node_modules/@antfu/node_modules:/Users/hibiki/CharlotteChenblog/Q-Blog/node_modules/.pnpm/@antfu+eslint-config@3.0.0_@typescript-eslint+utils@8.3.0_eslint@9.9.1_jiti@1.21.3__typescrip_rxgiowk5oe6yk2k63vvhvsky2i/node_modules:/Users/hibiki/CharlotteChenblog/Q-Blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
